{
  "manifest_version": 3,
  "name": "Window & Tab Stats",
  "version": "1.1.12",
  "permissions": ["tabs", "windows", "tabGroups"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icon/icon-48x48.png",
      "128": "icon/icon-128x128.png"
    }
  },
  "icons": {
    "48": "icon/icon-48x48.png",
    "128": "icon/icon-128x128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
